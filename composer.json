{
  "name": "mawoscha/german-localepack-es-co",
  "type": "magento-module",
  "license":"OSL-3.0",
  "description":"Spanish (Colombia) Community Translation",
  "authors":[
    {
      "name":"MaWoScha"
    }
  ],
  "require": {
    "magento-hackathon/magento-composer-installer": "*"
  },
  "extra": {
    "magento-root-dir": "./",
    "magento-deploystrategy": "copy",
    "map": [
      ["./app/code/community/German/LocalePackCo", "./app/code/community/German/LocalePackCo"],
      ["./app/etc/modules/German_LocalePackCo.xml", "./app/etc/modules/German_LocalePackCo.xml"],
      ["./app/locale/de_DE/German_LocaleCo.csv", "./app/locale/de_DE/German_LocaleCo.csv"],
      ["./app/locale/es_CO/German_LocaleCo.csv", "./app/locale/es_CO/German_LocaleCo.csv"],
      ["./app/locale/es_ES/German_LocaleCo.csv", "./app/locale/es_ES/German_LocaleCo.csv"],
      ["./app/locale/pt_BR/German_LocaleCo.csv", "./app/locale/pt_BR/German_LocaleCo.csv"]
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
